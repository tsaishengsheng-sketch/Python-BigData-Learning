   "source": [
    "\"\"\"\n",
    "檔名：print_number_pattern.py\n",
    "功能：迴圈演算法練習\n",
    "學習重點：for/while 迭代、控制流程\n",
    "\"\"\"\n",
    "for i in range(1, 10):\n",
    "    # 建立該行的初始空白\n",
    "    spaces = \" \" * (9 - i)\n",
    "    numbers = \"\"\n",
    "    for j in range(1, i + 1):\n",
    "        # 累加數字與空白\n",
    "        numbers += f\"{j} \"\n",
    "    \n",
    "    # 結合空白與數字後印出\n",
    "    print(f\"{spaces}{numbers}\")"
   ]
  }
