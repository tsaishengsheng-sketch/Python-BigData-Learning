   "source": [
    "\"\"\"\n",
    "檔名：lcm_finder.py\n",
    "功能：迴圈演算法練習\n",
    "學習重點：for/while 迭代、控制流程\n",
    "\"\"\"\n",
    "gcd = 1\n",
    "n = 2\n",
    "a = eval(input('Enter a number:'))\n",
    "b = eval(input('Enter a number:'))\n",
    "while n <= a and n <= b:\n",
    "    if a % n == 0 and b % n == 0:\n",
    "        gcd = n\n",
    "    n += 1\n",
    "print('GCD(%d,%d) = %d'%(a, b, gcd))"
   ]
  }
