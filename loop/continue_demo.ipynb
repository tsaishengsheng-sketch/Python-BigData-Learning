    "\"\"\"\n",
    "檔名：continue_demo.py\n",
    "功能：迴圈演算法練習\n",
    "學習重點：for/while 迭代、控制流程\n",
    "\"\"\"\n",
    "total = 0\n",
    "i = 1\n",
    "while i <= 100:\n",
    "    if i == 9 or i == 99:\n",
    "        i += 1\n",
    "        continue\n",
    "    else:\n",
    "        total += i\n",
    "        i += 1\n",
    "        print('total = %d'%(total))"
   ]
  }
