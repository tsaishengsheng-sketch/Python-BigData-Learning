    "\"\"\"\n",
    "total = 0\n",
    "i = 1\n",
    "while i <= 5:\n",
    "    a = eval(input('Enter a number: '))\n",
    "    total += a\n",
    "    i += 1\n",
    "print('The total is', total)"
