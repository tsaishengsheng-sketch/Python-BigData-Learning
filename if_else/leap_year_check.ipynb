    "\"\"\"\n",
    "檔名：leap_year_check.py\n",
    "功能：邏輯判斷練習\n",
    "學習重點：條件分歧、布林邏輯運算\n",
    "\"\"\"\n",
    "year = eval(input('Enter a year '))\n",
    "con1 = year % 400 == 0\n",
    "con2 = year % 4 == 0 \n",
    "con3 = year % 100 != 0\n",
    "if con1 or (con2 and con3):\n",
    "    print('%d is a leap year.'%(year))\n",
    "else:\n",
    "    print('%d is a common year.'%(year))"
   ]
  }
